<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>notes</title>
</head>
<body>
    <div class="notes content">

        <% if (notes.length > 0) { %>
         <% notes.forEach(note => { %>
            <p class="note"> <%= note.note %> </p>
         <% }); %>
        <% } %>
    </div>
</body>
</html>